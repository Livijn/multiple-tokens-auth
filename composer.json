{
    "name": "livijn/multiple-tokens-auth",
    "description": "Adds the ability to use multiple tokens for the auth:api middleware.",
    "homepage": "https://github.com/livijn/multiple-tokens-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Fredrik Livijn",
            "email": "ouff@live.se"
        }
    ],
    "require": {
        "php" : ">=7.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Livijn\\MultipleTokensAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Livijn\\MultipleTokensAuth\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Livijn\\MultipleTokensAuth\\MultipleTokensAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
